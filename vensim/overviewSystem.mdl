********************************************************
	.infrastructureSystem
********************************************************~
	|

Staff required for building infrastructure=
	0.1
	~	
	~		|

Plan new infrastructure=
	MAX(MIN(DELAY1(Infrastructure Shortage-Infrastructure Surplus+Infrastructure aging,Infrastructure planning time\
		), Production possible for planning activities/Staff required for building infrastructure
	), 0)
	~	Ml/(year*year)
	~		|

Build infrastructure=
	MAX(0, MIN(Planned infrastructure/Infrastructure building time ,Production possible for construction activities\
		/Staff required for building infrastructure))
	~	Ml/(year*year)
	~		|

percentage infrastructure maintained annually=
	MIN(MIN(Production possible for primary activities, Available maintenance capacity by funds\
		) / Theoretical Infrastructure Capacity
	 * 100, Target percentage infrastructure maintenance * 100)
	~	Dmnl
	~		|

Infrastructure Surplus=
	MAX(Practical infrastructure capacity-Total water demand, 0)
	~	Ml/year
	~		|

Leakage=
	0.3
	~	Dmnl
	~		|

Maximum capacity exceeding=
	0.3
	~	Dmnl
	~		|

Infrastructure Aging Time=
	30 + Effect of maintenance on aging time + Effect of infrastructure pushing on aging time
	~	year
	~		|

Effect of infrastructure pushing on aging time= WITH LOOKUP (
	Infrastructure Shortage/Practical infrastructure capacity,
		([(0,-10)-(30,10)],(0,0),(30,-10) ))
	~	Dmnl
	~		|

Effect of maintenance on aging time= WITH LOOKUP (
	percentage infrastructure maintained annually,
		([(0,-10)-(100,10)],(0,-10),(8,0),(100,10) ))
	~	Dmnl
	~		|

Maximum infrastructure capacity=
	Practical infrastructure capacity*(1+Maximum capacity exceeding)
	~	Ml / year
	~		|

Practical infrastructure capacity=
	Theoretical Infrastructure Capacity * (1-Leakage)
	~	Ml/year
	~		|

Production rate required for primary staff activities= ACTIVE INITIAL (
	Target percentage infrastructure maintenance*Theoretical Infrastructure Capacity,
		1)
	~	Ml/year
	~		|

Production rate required for secondary staff activities= ACTIVE INITIAL (
	MAX(0, (DELAY1(Infrastructure Shortage+Infrastructure aging-Infrastructure Surplus,Infrastructure planning time\
		)) + Planned infrastructure/Infrastructure building time),
		1)
	~	Ml/year
	~		|

Infrastructure aging=
	Theoretical Infrastructure Capacity/Infrastructure Aging Time
	~	Ml/(year*year)
	~	Theoretical Infrastructure Capacity/ Infrastructure Aging Time
	|

Infrastructure planning time=
	0.1
	~	year
	~		|

Target percentage infrastructure maintenance=
	0.08
	~	Dmnl
	~		|

Infrastructure building time=
	5
	~	year
	~		|

Theoretical Infrastructure Capacity= INTEG (
	Build infrastructure-Infrastructure aging,
		1825)
	~	Ml/year [0,100000]
	~		|

Infrastructure Shortage=
	MAX(Total water demand-Practical infrastructure capacity, 0)
	~	Ml/year
	~		|

Planned infrastructure= INTEG (
	Plan new infrastructure-Build infrastructure,
		1000)
	~	Ml/year [0,100000]
	~		|

********************************************************
	.crisisSystem
********************************************************~
	|

Delivery discrepancy connected households=
	Volume of water supplied by household service delivery/Total water demand connected households
	~	1
	~		|

Crisis discrepancy rate connected households=
	0
	~	Dmnl
	~		|

Crisis discrepancy rate unconnected households=
	0
	~	Dmnl
	~		|

Water crisis reached=
	IF THEN ELSE(Delivery discrepancy connected households<=Crisis discrepancy rate connected households\
		, 1, IF THEN ELSE(Delivery discrepancy other urban users<=Crisis discrepancy rate other urban users\
		, 1, IF THEN ELSE(Delivery discrepancy unconnected households<=Crisis discrepancy rate unconnected households\
		, 1, 0)))
	~	1
	~		|

Delivery discrepancy other urban users=
	Volume of water supplied for urban water users/Total water demand other urban users
	~	1
	~		|

Delivery discrepancy unconnected households=
	Volume of water supplied by household service delivery/Total water demand unconnected households
	~	1
	~		|

Crisis discrepancy rate other urban users=
	0
	~	Dmnl
	~		|

********************************************************
	.fundingSystem
********************************************************~
	|

Available maintenance capacity by funds=
	Available funds for maintenance/Average annual maintenance cost per unit of capacity
	~	Ml/year
	~		|

Volume of water supplied by household service delivery=
	IF THEN ELSE(Total water demand > Maximum infrastructure capacity, ((Total water demand connected households\
		+Total water demand unconnected households
	)/Total water demand) * Maximum infrastructure capacity , Total water demand connected households\
		+Total water demand unconnected households
	 )
	~	Ml/year
	~		|

Volume of water supplied for urban water users=
	IF THEN ELSE(Total water demand > Maximum infrastructure capacity, (Total water demand other urban users\
		/Total water demand) * Maximum infrastructure capacity , Total water demand other urban users\
		 )
	~	Ml/year
	~		|

"Annual financial bail-out"=
	150
	~	kZAR/year
	~		|

Available funds for maintenance= INTEG (
	Income flow-Maintenance expenditures,
		1000)
	~	kZAR
	~		|

Average annual maintenance cost per unit of capacity=
	2.5
	~	kZAR/(Ml/year)
	~		|

Percentage of household service delivery costs recovered=
	0.4
	~	Dmnl
	~		|

Percentage of other urban user service delivery costs recovered=
	0.75
	~	Dmnl
	~		|

Unit charge=
	1
	~	kZAR/Ml
	~		|

Maintenance expenditures=
	MIN(Theoretical Infrastructure Capacity*Average annual maintenance cost per unit of capacity\
		, Available funds for maintenance)
	~	kZAR/year
	~		|

Income flow=
	"Annual financial bail-out"+Municipality income
	~	kZAR/year
	~		|

Municipality income=
	((Percentage of household service delivery costs recovered*(Unit charge*Volume of water supplied by household service delivery
	))+(Percentage of other urban user service delivery costs recovered*(Unit charge*Volume of water supplied for urban water users
	)))
	~	kZAR/year
	~		|

